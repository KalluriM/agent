        "typescript": "^3.2.4"
    },
    "files": [
        "*.js",
        "build",
        "node_modules"
    ],
    "build": {
        "appId": "in.xema.manager",
        "copyright": "Copyright 2019 Xema",
        "publish": {
            "provider": "github"
        },
        "win": {
            "certificateFile": "private/xema.pfx",
            "certificatePassword": "",
            "verifyUpdateCodeSignature": true,
            "publisherName": "xema"
